

//User function Template for Java

class Solution
{
    public int[] countOddEven(int[] arr, int n)
    {
        // Code here
        
        int Even=0, Odd=0;
        for (int i =0; i <arr.length; i++) {
            if (arr[i]%2 == 0) {
                Even++;
            }
            else {
                Odd++;
            }
        }
        int[] rlt = {Odd,Even};
        return rlt;
        
    }
}
