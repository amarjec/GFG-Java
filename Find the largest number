
class Solution{
    static int find(int N){
        // code here 
         for(int i=N; i>=1; i--){
            if(isit(i)){
               return i; 
            }
        }
        return -1;
}
   
    private static boolean isit(int num){
        int ele=9;
        while(num>0){
           int digit=num%10;
           if(digit<=ele){
               ele=digit;
               num/= 10;
                }
                else return false;
            }
            return true;
    }
}
