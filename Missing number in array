

// User function Template for Java

class Solution {
    int missingNumber(int array[], int n) {
        // Your Code Here
       Arrays.sort(array);
        int total = n*(n+1)/2;
        int sum = 0;
        for (int i = 0; i<array.length; i++) {
            sum += array[i];
        }
        int output = total - sum;
        return output;
    }
}
