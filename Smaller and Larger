

//User function Template for Java




class Solve {
    int[] getMoreAndLess(int[] arr, int n, int x) {
        // code here
        int less = 0, more = 0;
        for (int i =0; i<n; i++) {
            if (arr[i]<=x) {
                less++;
            }
        }
        for (int i =0; i<n; i++) {
            if (arr[i]>=x) {
                more++;
            }
        }
        
        return new int[] {less,more};
    }
}
