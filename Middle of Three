

//User function Template for Java

class Solution{
    int middle(int A, int B, int C){
        //code here
          if(A<B && A>C || A>B && A<C)
        {
            return A;
        }
        else if(B>C && B<A || B<C && B>A)
        {
            return B;
        }
        else
        {
            return C;
        }
        
    }
};
    
