// User function Template for Java

class Solve {
    int[] findTwoElement(int arr[], int n) {
        // code here
        int a1[] ={-1,-1};
        int a [] = new int [n+1];
        for(int i=0;i<n;i++)
            a[arr[i]]++;
            
            for(int i=0;i<=n;i++){
                if(a[i]>1)
                 a1[0]=i;
                 if(a[i]==0)
                  a1[1]=i;
            }
            return a1;
   
    }
}
